{
  "manifest_version": 2,

  "name": "Facebokmark",
  "description": "This extension lets you bookmark facebook posts.",
  "version": "1.0",

  "permissions": [
    "https://secure.flickr.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
    "content_scripts": [
    {
      "matches": ["http://www.facebook.com/*"],
      "css": ["mystyles.css"],
      "js": ["facebook.js"]
    }
  ],

}